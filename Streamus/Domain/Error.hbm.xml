<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" assembly="Streamus" namespace="Streamus.Domain">
  
  <class name="Error" table="[Errors]" lazy="false">
    <id name="Id" unsaved-value="00000000-0000-0000-0000-000000000000">
      <generator class="guid.comb" />
    </id>
    
    <property name="Message" not-null="true"/>
    <property name="LineNumber" not-null="true"/>
    <property name="Url" not-null="true" />
    <property name="ClientVersion" not-null="true" />
    <property name="TimeOccurred" not-null="true" />

  </class>
  
</hibernate-mapping>